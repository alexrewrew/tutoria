@import "../../utilities/vars"
@import "../../utilities/mixins"

a
  text-decoration none
  transitions()
  &:hover
    text-decoration none
  &:focus
    text-decoration none
    outline none

.link-logo
  display block
  max-width 160px
  padding 20px 0 21px
  +mobile()
    max-width 100px
    padding: 14px 0 !important
    position relative
  img
    width 100%

.link-birds
  display block

.birds-mobile
  display none
  position absolute
  max-width: 70px;
  top 1px
  left 15px
  +mobile()
    display block

